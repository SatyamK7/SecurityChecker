{
    "manifest_version": 3,
    "name": "Authenticity Checker",
    "version": "1.0",
    "description": "Checks if the current site is authentic.",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.jpeg"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
          
        }
      ]
  }
  